<view><block wx:if="{{showWelcome==true}}"><view class="welcome padding-bottom-xl margin-bottom-xl" style="{{'height:'+(height)+';'}}"><view style="{{'background:url(\'/h5/static/Search_BG.png\') no-repeat center;background-size:cover;'+('height:'+(height)+';')}}"><image style="{{'width:90%;height:102%;position:absolute;left:calc(44rpx);border:#000 solid 0px;'+('animation:'+('show 1s 1')+';')}}" src="/static/Search_Button.png" mode="aspectFit" data-event-opts="{{[['tap',[['showContent',['$event']]]]]}}" bindtap="__e"></image></view></view></block><block wx:if="{{showWelcome==false}}"><view class="search-box" style="width:398.75px;"><block slot="content">首页</block><m-search class="mSearch-input-box" vue-id="1" mode="{{2}}" button="inside" placeholder="{{defaultKeyword}}" value="{{keyword}}" data-event-opts="{{[['^search',[['doSearch']]],['^input',[['__set_model',['','keyword','$event',[]]],['inputChange']]],['^confirm',[['doSearch',[false]]]],['^getFocus',[['showHistory']]]]}}" bind:search="__e" bind:input="__e" bind:confirm="__e" bind:getFocus="__e" bind:__l="__l"></m-search></view></block><view data-event-opts="{{[['touchstart',[['blur',['$event']]]]]}}" class="search-keyword" bindtouchstart="__e"><scroll-view hidden="{{!(isShowKeywordList)}}" class="keyword-box" scroll-y><block wx:if="{{oldKeywordList.length>0}}"><view class="keyword-block"><view class="keyword-list-header"><view>历史搜索</view><view><image src="/static/HM-search/delete.png" data-event-opts="{{[['tap',[['oldDelete',['$event']]]]]}}" bindtap="__e"></image></view></view><view class="keyword"><block wx:for="{{oldKeywordList}}" wx:for-item="keyword" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['doSearch',['$0'],[[['oldKeywordList','',index]]]]]]]}}" bindtap="__e">{{keyword}}</view></block></view></view></block><view class="keyword-block"><view class="keyword-list-header"><view>热门搜索</view><view><image src="{{'/static/HM-search/attention'+forbid+'.png'}}" data-event-opts="{{[['tap',[['hotToggle',['$event']]]]]}}" bindtap="__e"></image></view></view><block wx:if="{{forbid==''}}"><view class="keyword"><block wx:for="{{hotKeywordList}}" wx:for-item="keyword" wx:for-index="index" wx:key="index"><view data-event-opts="{{[['tap',[['doSearch',['$0'],[[['hotKeywordList','',index]]]]]]]}}" bindtap="__e">{{keyword}}</view></block></view></block><block wx:else><view class="hide-hot-tis"><view>当前搜热门搜索已隐藏</view></view></block></view></scroll-view></view><uni-popup class="vue-ref" vue-id="2" type="center" mask-click="{{false}}" animation="{{true}}" data-ref="popupEmpty" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-tip"><view class="uni-tip-title text-xl">暂无相关搜索结果<view class="_br"></view>请尝试更换您的关键词<view class="_br"></view>つ♡⊂<view class="_br"></view>———————————</view><view class="uni-tip-group-button"><text data-event-opts="{{[['tap',[['clearInput',['$event']]]]]}}" class="uni-tip-button text-xl" bindtap="__e">好的👌</text></view></view></uni-popup><uni-popup class="vue-ref" vue-id="3" type="center" mask-click="{{false}}" animation="{{true}}" data-ref="popupBottom" bind:__l="__l" vue-slots="{{['default']}}"><view class="uni-tip"><view class="uni-tip-title text-xl">已经到底啦<view class="_br"></view>つ♡⊂<view class="_br"></view>———————————</view><view class="uni-tip-group-button"><text data-event-opts="{{[['tap',[['exit',['$event']]]]]}}" class="uni-tip-button text-xl" bindtap="__e">好的👌</text></view></view></uni-popup></view>