<scroll-view scrollY class="scrollPage">
    <cu-custom bgImage="https://image.weilanwl.com/color2.0/plugin/cjkz2329.jpg">
        <view slot="content">
            <image mode="widthFix" src="/images/cjkz.png"></image>
        </view>
    </cu-custom>
    <view class="cu-card">
        <view bindtap="toChild" class="cu-item bg-img shadow-blur" data-url="{{item.url}}" style="background-image:url({{item.img}})" wx:for="{{list}}">
            <view class="cardTitle">
        {{item.title}}
      </view>
        </view>
    </view>
    <view class="cu-tabbar-height"></view>
</scroll-view>
